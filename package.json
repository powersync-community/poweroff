{
  "name": "example-with-tailwindcss",
  "type": "module",
  "scripts": {
    "dev:stack:up": "docker compose up -d",
    "dev:stack:down": "docker compose down",
    "dev:stack:logs": "docker compose logs -f powersync postgres_source postgres_storage",
    "dev:stack:reset": "docker compose down -v --remove-orphans",
    "dev": "mkdir -p logs && vinxi dev 2>&1 | tee logs/dev.log",
    "build": "vinxi build",
    "start": "vinxi start",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e:headless": "node scripts/run-playwright-skill.mjs scripts/e2e/workorders-headless.js",
    "test:e2e:headless:url": "TARGET_URL=http://localhost:3000 node scripts/run-playwright-skill.mjs scripts/e2e/workorders-headless.js"
  },
  "dependencies": {
    "@journeyapps/wa-sqlite": "^1.4.1",
    "@powersync/common": "^1.47.0",
    "@powersync/node": "^0.17.0",
    "@powersync/web": "^1.33.0",
    "@solidjs/router": "^0.15.0",
    "@solidjs/start": "^1.1.0",
    "@tanstack/db": "^0.5.25",
    "@tanstack/powersync-db-collection": "^0.1.29",
    "@tanstack/solid-db": "^0.2.5",
    "@types/pg": "^8.15.6",
    "better-sqlite3": "^12.6.2",
    "jose": "^6.1.0",
    "pg": "^8.16.3",
    "solid-js": "^1.9.5",
    "solid-marked": "^0.6.3",
    "vinxi": "^0.5.7",
    "yjs": "^13.6.27",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/vite": "^4.0.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@vitest/ui": "^2.1.8",
    "jsdom": "^25.0.1",
    "tailwindcss": "^4.0.7",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=22"
  }
}
